  "main": "index.ts",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "start:dev": "concurrently -n Client, Server \"npm start\" \"npm run start:dev:server\"",
    "build:prod": "webpack --env mode=production url=https://project-prod-server.vercel.app/",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "visual:report": "npm run visual:report:json && npm run visual:report:html",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "all": "npm run lint:ts:fix && npm run lint:scss:fix && npm run test:unit && npm run build-storybook && npm run test:ui && npm run visual:report",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/createSlice/index.js"
  },
